{
  "name": "ev-music",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "fast-average-color": "^9.4.0",
    "vanjs-core": "^1.5.0",
    "vanjs-ext": "^0.4.0"
  },
  "scripts": {
    "tailwind": "tailwindcss -o style.css -c ./tailwind.config.js",
    "dot": "node esbuild.config.js",
    "build": "esbuild ./automator.js --keep-names --drop:debugger --pure:console.log --tree-shaking=true --legal-comments=none --charset=utf8 --format=iife --bundle --platform=browser --minify --target=chrome131,safari18.2,edge131,firefox133,opera115 --outdir=./js/"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/preset-env": "^7.24.4",
    "@parcel/babel-preset-env": "^2.12.0",
    "@parcel/transformer-css": "^2.12.0",
    "autoprefixer": "^10.4.7",
    "parcel": "^2.12.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24"
  },
  "transformers": {
    "*.css": [
      "@parcel/transformer-css",
      {
        "cssModules": true
      }
    ]
  }
}
